@model SchoolProject.Models.Class
@{
    ViewBag.Title = "Show";
}

<a class="navlink" href="/Class/List">Back To Classes</a>
<h2>@Model.ClassName</h2>
<ul>
    <li>Class code: @Model.ClassCode</li>
    <li>Start Date: @Model.StartDate.ToString("MMMM dd, yyyy")</li>
    <li>Finish Date: @Model.FinishDate.ToString("MMMM dd, yyyy")</li>
</ul>